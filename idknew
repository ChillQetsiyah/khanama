Section:NewButton("DeleteChannel", "ButtonInfo", function()
	local Player = game.Players.LocalPlayer
	local Character = Player.Character

	local Channel = Character:WaitForChild("Channeling")
	Channel:Destroy()
end)

Section:NewButton("Vervain loop", "ButtonInfo", function()
	 local Player = game.Players.LocalPlayer
	local Character = Player.Character
	local Live = game.Workspace.Live
	local RunService = game:GetService("RunService")
	local Debounce = false

	while task.wait(2) do
		if not game.Players.LocalPlayer.Character:FindFirstChild("VervainBody") and game.Workspace.Live:FindFirstChild(game.Players.LocalPlayer.Character.Name) then
			local A_1 = "Magic"
			local A_2 = 
				{
					[1] = "VervainBody",
					[2] = game.Players.LocalPlayer.Character
				}
			local Event = game:GetService("ReplicatedStorage").Events.Witch
			Event:FireServer(A_1, A_2)
		end
	end
end)
Section:NewButton("Disable All rings", "ButtonInfo", function()
		local Plr = game.Players.LocalPlayer
	
		for i, Player in pairs (game.Players:GetPlayers()) do
			if Player.Character ~= nil and Player ~= Plr then
				local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
				if mag < 999999 then
					local Target = Player.Character or Player.CharacterAdded:Wait()
	
					local A_1 = "Magic"
					local A_2 = 
						{
							[1] = "DisableRing",
							[2] = Target
						}
					local Event = game:GetService("ReplicatedStorage").Events.Witch
					Event:FireServer(A_1, A_2)
	
	
				end
			end
		end
	end)


    print("Clicked")


	local Tab = Window:NewTab("Misc")
	local Section = Tab:NewSection("Misc")
	
	Section:NewButton("Respawn", "ButtonInfo", function()
			local Event = game:GetService("ReplicatedStorage").Events.Respawn
			Event:FireServer()
		end)
		print("Clicked")
	
	Section:NewButton("mortal respawn", "ButtonInfo", function()
		local LocalPlayer = game.Players.LocalPlayer
		local Character = LocalPlayer.Character
		local Humanoid = Character.Humanoid
		local Mouse = LocalPlayer:GetMouse()
		local Respawn = game:GetService("ReplicatedStorage").Events.Respawn
		Respawn:FireServer()
		wait(1)
		local Morph = game:GetService("ReplicatedStorage").Events.Morph
		Morph:FireServer("Mortal")
	end)
		print("Clicked")
		Section:NewButton("Wolf bite", "ButtonInfo", function()
			local Player = game.Players.LocalPlayer
	local Character = Player.Character
	local Humanoid = Character.Humanoid
	local UserInputService = game:GetService("UserInputService")
	local mouse = Player:GetMouse()
	--local Debounce = true
	local Key = 'q'
	local db = true

	UserInputService.InputBegan:Connect(function(input, IsTyping)
		if IsTyping then return end
		if input.KeyCode == Enum.KeyCode[Key] then
			local A_1 = "Bite"
			local A_2 = 
				{
					[1] = false, 
					[2] = mouse.Target.Parent
				}
			local Event = game:GetService("ReplicatedStorage").Events.Werewolf
			Event:FireServer(A_1, A_2)

			UserInputService.InputBegan:Connect(function(input, IsTyping)
				if IsTyping then return end
				if input.KeyCode == Enum.KeyCode[Key] then
					local A_1 = "Bite"
					local A_2 = 
						{
							[1] = false, 
							[2] = mouse.Target.Parent.Parent
						}
					local Event = game:GetService("ReplicatedStorage").Events.Werewolf
					Event:FireServer(A_1, A_2)
				end
			end)
		end
	end)
end)
Section:NewButton("remove fall damage", "ButtonInfo", function()
    local FallDamage = Rep.Events.FallDamage
    FallDamage:Destroy()
end)
Section:NewButton("Infinite Stamina", "ButtonInfo", function()
	local Player = game.Players.LocalPlayer
	local Character = Player.Character
	local Stamina = Character:WaitForChild("Stamina")
	local Max = Stamina.Max

	Max.Value = 9000000000000000
	wait()
	Stamina.Value = 9000000000000000
end)



local Section = Tab:NewSection("Qetsiyah")
Section:NewButton("goto qet", "ButtonInfo", function()
	local Player = game.Players.LocalPlayer.Character.HumanoidRootPart
	local Live = game.Workspace.Live 
	for _, child in pairs (Live:GetDescendants()) do
		if child.Name == "QetsiyahMove" then
			local Qetsiyah = child.Parent
			Player.CFrame = Qetsiyah.HumanoidRootPart.CFrame
		end
	end
end)
Section:NewButton("Heal Qet", "ButtonInfo", function()
	local A_1 = "Magic"
	local A_2 = 
		{
			[1] = "Invisibility"
		}
	local Event = game:GetService("ReplicatedStorage").Events.Witch
	Event:FireServer(A_1, A_2)

	local Player = game.Players.LocalPlayer.Character.HumanoidRootPart
	local Live = game.Workspace.Live 
	for _, child in pairs (Live:GetDescendants()) do
		if child.Name == "QetsiyahMove" then
			local Qetsiyah = child.Parent
			local A_1 = "Heal"
			local A_2 = 
				{
					[1] = Qetsiyah
				}
			local Event = game:GetService("ReplicatedStorage").Events.Vampire
			Event:FireServer(A_1, A_2)
		end
	end
end)
Section:NewButton("Push Qet", "ButtonInfo", function()
	local Live = game.Workspace.Live 
	for _, child in pairs (Live:GetDescendants()) do
		if child.Name == "QetsiyahMove" then
			local Qetsiyah = child.Parent
			local A_1 = "Push"
			local A_2 = 
				{
					[1] = Qetsiyah
				}
			local Event = game:GetService("ReplicatedStorage").Events.Witch
			Event:FireServer(A_1, A_2)
		end
	end
end)
Section:NewButton("Push Qets dead body", "ButtonInfo", function()
	local Live = game.Workspace.Live 
	for _, child in pairs (Live:GetDescendants()) do
		if child.Name == "QetsiyahMove" then
			local Qetsiyah = child.Parent
			local A_1 = "Magic"
			local A_2 = 
				{
					[1] = "Motus",
					[2] = true,
					[3] = game:GetService("Workspace").deathClones[(Qetsiyah.Name)..("deathClone")]
				}
			local Event = game:GetService("ReplicatedStorage").Events.Witch
			Event:FireServer(A_1, A_2)
		end
	end
end)
